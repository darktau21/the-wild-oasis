{
  "name": "the-wild-oasis",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "yarn@3.6.3",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "visualize:tree": "vite-bundle-visualizer --template treemap",
    "visualize:network": "rollup-plugin-visualizer --template network",
    "visualize:sunburst": "rollup-plugin-visualizer --template sunburst",
    "preview": "vite preview",
    "depgraph": "dependency-cruiser --include-only \"^src\" --config .dependency-cruiser.cjs --output-type ddot -- src | dot -T svg > folder-level-dependency-graph.svg",
    "lint:ts": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:css": "stylelint './src/**/*.tsx' --fix",
    "sb:start": "supabase start",
    "sb:stop": "supabase stop",
    "sb:gentypes": "supabase gen types typescript --local > ./src/shared/api/apiTypes.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.33.1",
    "@tanstack/react-query": "^4.33.0",
    "nanoid": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.4",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.10.1",
    "react-router-dom": "^6.15.0",
    "styled-components": "^6.0.7"
  },
  "devDependencies": {
    "@feature-sliced/eslint-config": "^0.1.0-beta.6",
    "@logux/stylelint-config": "^0.16.0",
    "@tanstack/eslint-plugin-query": "^4.32.5",
    "@tanstack/react-query-devtools": "^4.33.0",
    "@types/node": "^20.5.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.15",
    "dependency-cruiser": "^13.1.4",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-boundaries": "^3.4.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-perfectionist": "^1.5.1",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.29",
    "postcss-styled-syntax": "^0.4.0",
    "prettier": "^3.0.2",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "supabase": "^1.88.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-bundle-visualizer": "^0.10.0",
    "vite-plugin-checker": "^0.6.2"
  }
}
